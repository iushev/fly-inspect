{
    "python.venvPath": "${workspaceRoot}/api_python/.venv",
    "python.pythonPath": "${workspaceRoot}/api_python/.venv/bin/python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true
    },
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ]
}